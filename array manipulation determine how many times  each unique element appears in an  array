#include <stdio.h>
int main() {
    int n, arr[100];
    int freq[100] = {0};
    printf("Enter number of elements: ");
    scanf("%d", &n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        freq[arr[i]]++;
    }
    printf("Frequency of each element:\n");
    for (int i = 0; i < n; i++) {
        if (freq[arr[i]] != 0) {
            printf("%d occurs %d times\n", arr[i], freq[arr[i]]);
            freq[arr[i]] = 0;  
        }
    }
    return 0;
}
